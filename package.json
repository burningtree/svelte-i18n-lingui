{
	"name": "svelte-i18n-lingui",
	"version": "0.1.7",
	"author": "Henry Roes Lie",
	"license": "MIT",
	"description": "Add i18n to Svelte/Sveltekit projects using Lingui, using message as the catalog id",
	"repository": "github:HenryLie/svelte-i18n-lingui",
	"keywords": [
		"svelte",
		"sveltekit",
		"i18n",
		"l10n",
		"lingui",
		"multilingual",
		"translation",
		"internationalization",
		"localization"
	],
	"scripts": {
		"prepare": "svelte-kit sync && npm run build",
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test": "vitest",
		"extract": "lingui extract",
		"compile": "lingui compile --typescript"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./extractor": {
			"types": "./dist/extractor.d.ts",
			"default": "./dist/extractor.js"
		}
	},
	"typesVersions": {
		">4.0": {
			"extractor": [
				"./dist/extractor.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@lingui/core": "^4.5.0",
		"svelte": "5.x"
	},
	"devDependencies": {
		"@lingui/cli": "^4.10.0",
		"@lingui/conf": "^4.10.0",
		"@lingui/core": "^4.10.0",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.30.4",
		"@sveltejs/package": "^2.3.1",
		"@sveltejs/vite-plugin-svelte": "^2.5.3",
		"@testing-library/svelte": "^4.2.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.38.0",
		"estree-walker": "^3.0.3",
		"jsdom": "^22.1.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"publint": "^0.2.7",
		"svelte": "5.0.0-next.120",
		"svelte-check": "^3.7.0",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"typescript-svelte-plugin": "^0.3.38",
		"vite": "^4.5.3",
		"vitest": "^0.34.6"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@typescript-eslint/typescript-estree": "^6.21.0",
		"js-sha256": "^0.10.1",
		"svelte-preprocess": "^5.1.4"
	}
}
